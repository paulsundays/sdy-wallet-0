{
  "expo": {
    "name": "Sundays Wallet",
    "slug": "sundays-wallet",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pdominguez.sundayswallet",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "icon": "./assets/images/sundays-icon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/sundays-icon.png",
        "backgroundColor": "#ffffff",
        "backgroundImage": "./assets/images/sundays-icon.png"
      },
      "package": "com.pdominguez.sundayswallet",
      "versionCode": 3
    },
    "submit": {
      "production": {
        "ios": {
          "ascAppId": "6747622064"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "splash": {
      "image": "./assets/images/sundays-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "fbbfcb7b-cd6c-4a38-bdea-f516b2448f04"
      }
    },
    "owner": "pdominguez0998expo"
  }
}
